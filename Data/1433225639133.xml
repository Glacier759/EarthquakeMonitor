  <script type="text/javascript">function jsonpCallback(wthBlock) {	var city = wthBlock.data.city[0].info.name;	var dayWth = wthBlock.data.city[0].days.day[0].s1; //白天天描述	var dayTemp = wthBlock.data.city[0].days.day[0].t1; //白天温度	var dayIco = wthBlock.data.city[0].days.day[0].f1; //白天天气图标	var nightWth = wthBlock.data.city[0].days.day[0].s2; //晚上	var nightTemp = wthBlock.data.city[0].days.day[0].t2; //温度	var nightIco = wthBlock.data.city[0].days.day[0].f2; //晚上	var tmp = nightTemp.substring(0, nightTemp.length - 1) + "~" + dayTemp; //温差	var dayFengXiang = "";	var dayFengSu = "";	var tianQi = "";	var time = new Date();	var hours = time.getHours(); //获取时间的小时	if (hours >= 19 || hours <= 7) {		var dayFengXiang = wthBlock.data.city[0].days.day[0].d2;		var dayFengSu = wthBlock.data.city[0].days.day[0].p2;		tianQi = nightWth;	} else {		var dayFengXiang = wthBlock.data.city[0].days.day[0].d1;		var dayFengSu = wthBlock.data.city[0].days.day[0].p1;		tianQi = dayWth;	}	document.getElementById("wth_cityoneico").innerHTML = dayWth; //白天	document.getElementById("wth_cityoneico").className = "wth-sty-lev wth-Bg wth-one-ico pt " + dayIco;	document.getElementById("wth_cityTwoico").innerHTML = nightWth; //晚上	document.getElementById("wth_cityTwoico").className = "wth-sty-lev wth-Bg wth-one-ico pt " + nightIco + "1";	document.getElementById("wth_cityTianQiStr").innerHTML = tianQi; //天气	document.getElementById("wth_cityName").innerHTML = city;	document.getElementById("wth_cityWenDutemp").innerHTML = tmp;	document.getElementById("wth_cityFengSu").innerHTML = dayFengXiang + dayFengSu;} </script> 
