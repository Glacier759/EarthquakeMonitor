 += '<div class="comment-author" style="margin-left:0px;width:60px; height:60px; float:left;">';		html += '<img width=\'60px\' height=\'60px\' src="'+imgurl+v.img+'"  onerror="this.src=\'../img/defaulthead-60x60.jpg\'">';		html += '</div>';		html += '<div class="comment-content" style="float:left;width:550px;">';		html += '<div class=\'bcommentmsg global_text1\' title="'+v.comment+'" style="text-align:left;padding:0;border:0;height:40px;overflow:hidden;line-height:20px;">';		if(v.comment){			html += v.comment;		}else{			html += '没有评论';		}		html += '</div>';		html += '<div class="bcmsgdb global_info4">'+v.user_name+'发表于'+v.time+'';		html += '</div>';		html += '</div>';		html += '</div>';	});  	$("#comment_all_list").html(html);	$("#page").html(obj.txtpage);					   }		function success(obj){	 createlist(obj);}</script>
